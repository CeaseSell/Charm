    background-color: #222;
    color: white;
    text-align: center;
    margin: 10px auto;
}

.belly-table th, .belly-table td {
    padding: 10px;
    border: 1px solid #555;
    width: 25%;
}

/* Rewards Table */
.rewards-table {
    width: 80%;
    border-collapse: collapse;
    background-color: #222;
    color: white;
    text-align: center;
    margin: 10px auto;
}

.rewards-table th,
.rewards-table td {
    padding: 10px;
    border: 1px solid #555;
    width: 25%;
}

