            <span class="hljs-keyword">const</span> eventRewardsTable = doc.<span class="hljs-title function_">querySelector</span>(<span class="hljs-string">"table.wikitable"</span>); <span class="hljs-comment">// Gets first table</span>
            <span class="hljs-keyword">const</span> clanRewardsTable = doc.<span class="hljs-title function_">querySelectorAll</span>(<span class="hljs-string">"table.wikitable"</span>)[<span class="hljs-number">1</span>]; <span class="hljs-comment">// Gets second table</span>

            <span class="hljs-keyword">if</span> (eventRewardsTable &amp;&amp; clanRewardsTable) {
                <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">"eventRewards"</span>).<span class="hljs-property">innerHTML</span> = eventRewardsTable.<span class="hljs-property">innerHTML</span>;
                <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">"clanRewards"</span>).<span class="hljs-property">innerHTML</span> = clanRewardsTable.<span class="hljs-property">innerHTML</span>;
            } <span class="hljs-keyword">else</span> {
                <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">"eventRewards"</span>).<span class="hljs-property">innerHTML</span> = <span class="hljs-string">"&lt;tr&gt;&lt;td&gt;No rewards found&lt;/td&gt;&lt;/tr&gt;"</span>;
