<span class="hljs-comment">// 🛠 Change Owned Motes Count</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">changeMotes</span>(<span class="hljs-params">direction</span>) {
    <span class="hljs-keyword">const</span> owned = <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">"motesOwned"</span>);
