<span class="hljs-comment">// 🔹 Deselect All Rewards</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">deselectAll</span>(<span class="hljs-params">containerId</span>) {
    <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">querySelectorAll</span>(<span class="hljs-string">`#<span class="hljs-subst">${containerId}</span> tr.selected`</span>).<span class="hljs-title function_">forEach</span>(<span class="hljs-function"><span class="hljs-params">row</span> =&gt;</span> {
