<span class="hljs-comment">// Fetch Rewards from Warframe Wiki API (Using Proxy)</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">fetchRewards</span>(<span class="hljs-params"></span>) {
    <span class="hljs-keyword">const</span> proxyUrl = <span class="hljs-string">"https://corsproxy.io/?"</span>;
