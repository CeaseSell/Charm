// 🔹 Setup the Mote Tracker Table
function setupMoteTracker() {
    const bellyDiv = document.getElementById("belly-tracker");

    bellyDiv.innerHTML = <code>
</code>        &lt;table class="belly-table"&gt;
            &lt;thead&gt;
                &lt;tr&gt;
                    &lt;th&gt;Goal&lt;/th&gt;
                    &lt;th&gt;Needed&lt;/th&gt;
                    &lt;th&gt;Owned&lt;/th&gt;
                    &lt;th&gt;Change&lt;/th&gt;
                &lt;/tr&gt;
            &lt;/thead&gt;
            &lt;tbody&gt;
                &lt;tr&gt;
                    &lt;td&gt;&lt;input type="number" id="motesGoal" value="0" min="0"&gt;&lt;/td&gt;
                    &lt;td&gt;&lt;span id="motesNeeded" class="number-box"&gt;0&lt;/span&gt;&lt;/td&gt;
                    &lt;td&gt;&lt;span id="motesOwned" class="number-box"&gt;0&lt;/span&gt;&lt;/td&gt;
                    &lt;td&gt;
                        &lt;input type="number" id="motesAmount" value="0" min="1"&gt;
                        &lt;button class="mote-btn" onclick="changeMotes(1)"&gt;+&lt;/button&gt;
                        &lt;button class="mote-btn" onclick="changeMotes(-1)"&gt;-&lt;/button&gt;
                    &lt;/td&gt;
                &lt;/tr&gt;
            &lt;/tbody&gt;
        &lt;/table&gt;
        &lt;button id="toggleRewards" onclick="toggleRewards()"&gt;Show Rewards&lt;/button&gt;
        &lt;div id="rewardsContainer" style="display: none;"&gt;
            &lt;h2&gt;Event Rewards List&lt;/h2&gt;
            &lt;div id="eventRewardsGrid" class="rewards-grid"&gt;&lt;/div&gt;

            &lt;h2&gt;Clan Rewards List&lt;/h2&gt;
            &lt;div id="clanRewardsGrid" class="rewards-grid"&gt;&lt;/div&gt;
        &lt;/div&gt;
    <code>;
</code>
    document.getElementById("motesGoal").addEventListener("input", updateMotes);
}

// 🔹 Change Owned Motes Count
function changeMotes(direction) {
    const owned = document.getElementById("motesOwned");
    const inputAmount = parseInt(document.getElementById("motesAmount").value) || 1;
    let newOwned = parseInt(owned.textContent) + (direction * inputAmount);

    owned.textContent = Math.max(newOwned, 0);
    updateMotes();
}

// 🔹 Update Needed Motes
function updateMotes() {
    const owned = parseInt(document.getElementById("motesOwned").textContent) || 0;
    const goal = parseInt(document.getElementById("motesGoal").value) || 0;
    document.getElementById("motesNeeded").textContent = Math.max(goal - owned, 0);
}

// 🔹 Show/Hide Rewards Table
function toggleRewards() {
    const container = document.getElementById("rewardsContainer");
    const button = document.getElementById("toggleRewards");

    if (container.style.display === "none") {
        container.style.display = "block";
        button.textContent = "Hide Rewards";
    } else {
        container.style.display = "none";
        button.textContent = "Show Rewards";
    }
}

// 🔹 Fetch Event &amp; Clan Rewards from Warframe Wiki
function fetchRewards() {
    const proxyUrl = "https://api.allorigins.win/get?url=";
    const wikiUrl = encodeURIComponent("https://warframe.fandom.com/api.php?action=parse&amp;page=Operation:_Belly_of_the_Beast&amp;format=json");

    fetch(proxyUrl + wikiUrl)
        .then(response =&gt; response.json())
        .then(data =&gt; {
            if (!data || !data.contents) {
                console.error("Invalid response from API.");
                return;
            }

            const parsedData = JSON.parse(data.contents);
            if (!parsedData.parse || !parsedData.parse.text) {
                console.error("Missing 'parse.text' in API response.");
                return;
            }

            const parser = new DOMParser();
            const doc = parser.parseFromString(parsedData.parse.text["*"], "text/html");

            // Locate reward tables
            const tables = doc.querySelectorAll("table.wikitable");
            console.log(<code>Found ${tables.length} tables on the wiki page.</code>);

            if (tables.length &gt;= 2) {
                processRewardsTable(tables[0], "eventRewardsGrid");
                processRewardsTable(tables[1], "clanRewardsGrid");
            } else {
                console.error("No reward tables found on Warframe Wiki.");
            }
        })
        .catch(error =&gt; console.error("Error fetching rewards:", error));
}

// 🔹 Process the Reward Tables and Display in Grid Format
function processRewardsTable(table, containerId) {
    const rows = table.querySelectorAll("tr");
    const grid = document.getElementById(containerId);

    rows.forEach((row, index) =&gt; {
        if (index === 0) return; // Skip header row

        const columns = row.querySelectorAll("td");
        if (columns.length &gt;= 2) {
            const rewardName = columns[0].textContent.trim();
            const rewardCost = parseInt(columns[1].textContent.trim()) || 0;

            // Fetch image from wiki
            const image = columns[0].querySelector("img");
            const imgSrc = image ? image.src : "default.png";

            const rewardDiv = document.createElement("div");
            rewardDiv.classList.add("reward-item");
            rewardDiv.innerHTML = <code>
</code>                &lt;img src="${imgSrc}" alt="${rewardName}"&gt;
                &lt;p&gt;${rewardName}&lt;/p&gt;
                &lt;p class="mote-cost"&gt;${rewardCost} Motes&lt;/p&gt;
            <code>;
</code>            rewardDiv.addEventListener("click", function () {
                this.classList.toggle("selected");
                updateMotesFromRewards();
            });

            grid.appendChild(rewardDiv);
        }
    });
}

// 🔹 Update Motes from Selected Rewards
function updateMotesFromRewards() {
    let totalMotes = 0;
    document.querySelectorAll(".selected .mote-cost").forEach(cost =&gt; {
        totalMotes += parseInt(cost.textContent) || 0;
    });

    document.getElementById("motesNeeded").textContent = totalMotes;
