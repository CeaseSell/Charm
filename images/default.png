            &lt;p class="mote-cost"&gt;<span class="hljs-subst">${reward.cost}</span> Motes&lt;/p&gt;
        `</span>;

        rewardDiv.<span class="hljs-title function_">addEventListener</span>(<span class="hljs-string">"click"</span>, <span class="hljs-keyword">function</span> (<span class="hljs-params"></span>) {
            <span class="hljs-variable language_">this</span>.<span class="hljs-property">classList</span>.<span class="hljs-title function_">toggle</span>(<span class="hljs-string">"selected"</span>);
            <span class="hljs-title function_">updateNeededMotes</span>();
