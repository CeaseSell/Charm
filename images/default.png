/* Global Dark Theme */
body {
    background-color: #1e1e1e;
    color: white;
    font-family: Arial, sans-serif;
    margin: 0;
    padding: 0;
}

/* Center All Content */
#content {
    display: flex;
    flex-direction: column;
    align-items: center;
    text-align: center;
    padding: 20px;
}

/* Tabs Styling */
#tabs {
    display: flex;
    justify-content: center;
    background-color: #3c3c3c;
    padding: 10px;
    border-bottom: 2px solid #555;
}

#tabs button {
    margin-right: 10px;
    padding: 10px;
    border: none;
    cursor: pointer;
    color: white;
    background-color: #444;
    font-size: 16px;
}

#tabs button:hover {
    background-color: #555;
}

/* Tab Content */
.tab {
    display: none;
    width: 80%;
}

.tab.active {
    display: block;
    margin: auto;
}

/* Lich/Sister Grid Styling */
.lich-container, .sister-container {
    text-align: center;
    padding: 20px;
}

.lich-title, .sister-title {
    font-size: 24px;
    font-weight: bold;
    padding: 10px;
    text-align: center;
    width: 100%;
}

.lich-title {
    background-color: red;
}

.sister-title {
    background-color: blue;
}

/* Grid for Mods */
.mod-grid {
    display: grid;
    grid-template-columns: repeat(3, 80px);
    gap: 10px;
    justify-content: center;
    padding: 10px;
}

.mod-box {
    width: 80px;
    height: 80px;
    background-color: #333;
    display: flex;
    align-items: center;
    justify-content: center;
    border: 2px solid #555;
}

.mod-box img {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

/* Buttons */
.lich-button, .sister-button {
    background-color: #444;
    color: white;
    border: none;
    padding: 8px 12px;
    cursor: pointer;
    font-size: 14px;
}

.lich-button:hover, .sister-button:hover {
    background-color: #555;
}

/* Row &amp; Grid Layout */
.lich-row, .sister-row {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 10px;
    padding: 5px;
}

.lich-row select, .sister-row select {
    padding: 5px;
    background-color: #222;
    color: white;
    border: 1px solid #555;
    font-size: 14px;
}

/* Belly of the Beast Theme */
.belly-title {
    text-align: center;
    font-size: 24px;
    font-weight: bold;
    background-color: #333;
    color: #9acd32; /* Jade Green */
    padding: 10px;
    width: 80%;
}

/* Belly of the Beast Mote Tracker */
.belly-table {
    width: 80%;
