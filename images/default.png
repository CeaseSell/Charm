    <span class="hljs-title function_">setupMoteTracker</span>();
    <span class="hljs-title function_">fetchRewards</span>();
});

<span class="hljs-comment">// 🔹 Setup the Mote Tracker Table</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">setupMoteTracker</span>(<span class="hljs-params"></span>) {
    <span class="hljs-keyword">const</span> bellyDiv = <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">"belly-tracker"</span>);

    bellyDiv.<span class="hljs-property">innerHTML</span> = <span class="hljs-string">`
        &lt;table class="belly-table"&gt;
            &lt;thead&gt;
                &lt;tr&gt;
                    &lt;th&gt;Goal&lt;/th&gt;
                    &lt;th&gt;Needed&lt;/th&gt;
                    &lt;th&gt;Owned&lt;/th&gt;
                    &lt;th&gt;Change&lt;/th&gt;
                &lt;/tr&gt;
            &lt;/thead&gt;
            &lt;tbody&gt;
                &lt;tr&gt;
                    &lt;td&gt;&lt;input type="number" id="motesGoal" value="0" min="0"&gt;&lt;/td&gt;
                    &lt;td&gt;&lt;span id="motesNeeded" class="number-box"&gt;0&lt;/span&gt;&lt;/td&gt;
                    &lt;td&gt;&lt;span id="motesOwned" class="number-box"&gt;0&lt;/span&gt;&lt;/td&gt;
                    &lt;td&gt;
                        &lt;input type="number" id="motesAmount" value="0" min="1"&gt;
                        &lt;button class="mote-btn" onclick="changeMotes(1)"&gt;+&lt;/button&gt;
                        &lt;button class="mote-btn" onclick="changeMotes(-1)"&gt;-&lt;/button&gt;
                    &lt;/td&gt;
                &lt;/tr&gt;
            &lt;/tbody&gt;
        &lt;/table&gt;
        &lt;button id="toggleRewards" onclick="toggleRewards()"&gt;Show Rewards&lt;/button&gt;
        &lt;div id="rewardsContainer" style="display: none;"&gt;
            &lt;h2&gt;Event Rewards List&lt;/h2&gt;
            &lt;div id="eventRewardsGrid" class="rewards-grid"&gt;&lt;/div&gt;

            &lt;h2&gt;Clan Rewards List&lt;/h2&gt;
            &lt;div id="clanRewardsGrid" class="rewards-grid"&gt;&lt;/div&gt;
        &lt;/div&gt;
    `</span>;

    <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">"motesGoal"</span>).<span class="hljs-title function_">addEventListener</span>(<span class="hljs-string">"input"</span>, updateMotes);
}

<span class="hljs-comment">// 🔹 Change Owned Motes Count</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">changeMotes</span>(<span class="hljs-params">direction</span>) {
    <span class="hljs-keyword">const</span> owned = <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">"motesOwned"</span>);
    <span class="hljs-keyword">const</span> inputAmount = <span class="hljs-built_in">parseInt</span>(<span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">"motesAmount"</span>).<span class="hljs-property">value</span>) || <span class="hljs-number">1</span>;
    <span class="hljs-keyword">let</span> newOwned = <span class="hljs-built_in">parseInt</span>(owned.<span class="hljs-property">textContent</span>) + (direction * inputAmount);

    owned.<span class="hljs-property">textContent</span> = <span class="hljs-title class_">Math</span>.<span class="hljs-title function_">max</span>(newOwned, <span class="hljs-number">0</span>);
    <span class="hljs-title function_">updateMotes</span>();
}

<span class="hljs-comment">// 🔹 Update Needed Motes</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">updateMotes</span>(<span class="hljs-params"></span>) {
    <span class="hljs-keyword">const</span> owned = <span class="hljs-built_in">parseInt</span>(<span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">"motesOwned"</span>).<span class="hljs-property">textContent</span>) || <span class="hljs-number">0</span>;
    <span class="hljs-keyword">const</span> goal = <span class="hljs-built_in">parseInt</span>(<span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">"motesGoal"</span>).<span class="hljs-property">value</span>) || <span class="hljs-number">0</span>;
    <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">"motesNeeded"</span>).<span class="hljs-property">textContent</span> = <span class="hljs-title class_">Math</span>.<span class="hljs-title function_">max</span>(goal - owned, <span class="hljs-number">0</span>);
}

<span class="hljs-comment">// 🔹 Show/Hide Rewards Table</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">toggleRewards</span>(<span class="hljs-params"></span>) {
    <span class="hljs-keyword">const</span> container = <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">"rewardsContainer"</span>);
    <span class="hljs-keyword">const</span> button = <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">"toggleRewards"</span>);

    <span class="hljs-keyword">if</span> (container.<span class="hljs-property">style</span>.<span class="hljs-property">display</span> === <span class="hljs-string">"none"</span>) {
        container.<span class="hljs-property">style</span>.<span class="hljs-property">display</span> = <span class="hljs-string">"block"</span>;
        button.<span class="hljs-property">textContent</span> = <span class="hljs-string">"Hide Rewards"</span>;
    } <span class="hljs-keyword">else</span> {
        container.<span class="hljs-property">style</span>.<span class="hljs-property">display</span> = <span class="hljs-string">"none"</span>;
        button.<span class="hljs-property">textContent</span> = <span class="hljs-string">"Show Rewards"</span>;
    }
}

<span class="hljs-comment">// 🔹 Fetch Event &amp; Clan Rewards from Warframe Wiki</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">fetchRewards</span>(<span class="hljs-params"></span>) {
    <span class="hljs-keyword">const</span> proxyUrl = <span class="hljs-string">"https://api.allorigins.win/get?url="</span>;
    <span class="hljs-keyword">const</span> wikiUrl = <span class="hljs-built_in">encodeURIComponent</span>(<span class="hljs-string">"https://warframe.fandom.com/api.php?action=parse&amp;page=Operation:_Belly_of_the_Beast&amp;format=json"</span>);

    <span class="hljs-title function_">fetch</span>(proxyUrl + wikiUrl)
        .<span class="hljs-title function_">then</span>(<span class="hljs-function"><span class="hljs-params">response</span> =&gt;</span> response.<span class="hljs-title function_">json</span>())
        .<span class="hljs-title function_">then</span>(<span class="hljs-function"><span class="hljs-params">data</span> =&gt;</span> {
            <span class="hljs-keyword">if</span> (!data || !data.<span class="hljs-property">contents</span>) {
                <span class="hljs-variable language_">console</span>.<span class="hljs-title function_">error</span>(<span class="hljs-string">"Invalid response from API."</span>);
                <span class="hljs-keyword">return</span>;
            }

            <span class="hljs-keyword">const</span> parsedData = <span class="hljs-title class_">JSON</span>.<span class="hljs-title function_">parse</span>(data.<span class="hljs-property">contents</span>);
            <span class="hljs-keyword">if</span> (!parsedData.<span class="hljs-property">parse</span> || !parsedData.<span class="hljs-property">parse</span>.<span class="hljs-property">text</span>) {
                <span class="hljs-variable language_">console</span>.<span class="hljs-title function_">error</span>(<span class="hljs-string">"Missing 'parse.text' in API response."</span>);
                <span class="hljs-keyword">return</span>;
            }

            <span class="hljs-keyword">const</span> parser = <span class="hljs-keyword">new</span> <span class="hljs-title class_">DOMParser</span>();
            <span class="hljs-keyword">const</span> doc = parser.<span class="hljs-title function_">parseFromString</span>(parsedData.<span class="hljs-property">parse</span>.<span class="hljs-property">text</span>[<span class="hljs-string">"*"</span>], <span class="hljs-string">"text/html"</span>);

            <span class="hljs-comment">// Locate reward tables</span>
            <span class="hljs-keyword">const</span> tables = doc.<span class="hljs-title function_">querySelectorAll</span>(<span class="hljs-string">"table.wikitable"</span>);
            <span class="hljs-variable language_">console</span>.<span class="hljs-title function_">log</span>(<span class="hljs-string">`Found <span class="hljs-subst">${tables.length}</span> tables on the wiki page.`</span>);

            <span class="hljs-keyword">if</span> (tables.<span class="hljs-property">length</span> &gt;= <span class="hljs-number">2</span>) {
                <span class="hljs-title function_">processRewardsTable</span>(tables[<span class="hljs-number">0</span>], <span class="hljs-string">"eventRewardsGrid"</span>);
                <span class="hljs-title function_">processRewardsTable</span>(tables[<span class="hljs-number">1</span>], <span class="hljs-string">"clanRewardsGrid"</span>);
            } <span class="hljs-keyword">else</span> {
                <span class="hljs-variable language_">console</span>.<span class="hljs-title function_">error</span>(<span class="hljs-string">"No reward tables found on Warframe Wiki."</span>);
            }
        })
        .<span class="hljs-title function_">catch</span>(<span class="hljs-function"><span class="hljs-params">error</span> =&gt;</span> <span class="hljs-variable language_">console</span>.<span class="hljs-title function_">error</span>(<span class="hljs-string">"Error fetching rewards:"</span>, error));
}

<span class="hljs-comment">// 🔹 Process the Reward Tables and Display in Grid Format</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">processRewardsTable</span>(<span class="hljs-params">table, containerId</span>) {
    <span class="hljs-keyword">const</span> rows = table.<span class="hljs-title function_">querySelectorAll</span>(<span class="hljs-string">"tr"</span>);
    <span class="hljs-keyword">const</span> grid = <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(containerId);

    rows.<span class="hljs-title function_">forEach</span>(<span class="hljs-function">(<span class="hljs-params">row, index</span>) =&gt;</span> {
        <span class="hljs-keyword">if</span> (index === <span class="hljs-number">0</span>) <span class="hljs-keyword">return</span>; <span class="hljs-comment">// Skip header row</span>

        <span class="hljs-keyword">const</span> columns = row.<span class="hljs-title function_">querySelectorAll</span>(<span class="hljs-string">"td"</span>);
        <span class="hljs-keyword">if</span> (columns.<span class="hljs-property">length</span> &gt;= <span class="hljs-number">2</span>) {
            <span class="hljs-keyword">const</span> rewardName = columns[<span class="hljs-number">0</span>].<span class="hljs-property">textContent</span>.<span class="hljs-title function_">trim</span>();
            <span class="hljs-keyword">const</span> rewardCost = <span class="hljs-built_in">parseInt</span>(columns[<span class="hljs-number">1</span>].<span class="hljs-property">textContent</span>.<span class="hljs-title function_">trim</span>()) || <span class="hljs-number">0</span>;

            <span class="hljs-comment">// Fetch image from wiki</span>
            <span class="hljs-keyword">const</span> image = columns[<span class="hljs-number">0</span>].<span class="hljs-title function_">querySelector</span>(<span class="hljs-string">"img"</span>);
            <span class="hljs-keyword">const</span> imgSrc = image ? image.<span class="hljs-property">src</span> : <span class="hljs-string">"default.png"</span>;

            <span class="hljs-keyword">const</span> rewardDiv = <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">createElement</span>(<span class="hljs-string">"div"</span>);
            rewardDiv.<span class="hljs-property">classList</span>.<span class="hljs-title function_">add</span>(<span class="hljs-string">"reward-item"</span>);
            rewardDiv.<span class="hljs-property">innerHTML</span> = <span class="hljs-string">`
                &lt;img src="<span class="hljs-subst">${imgSrc}</span>" alt="<span class="hljs-subst">${rewardName}</span>"&gt;
                &lt;p&gt;<span class="hljs-subst">${rewardName}</span>&lt;/p&gt;
                &lt;p class="mote-cost"&gt;<span class="hljs-subst">${rewardCost}</span> Motes&lt;/p&gt;
            `</span>;
            rewardDiv.<span class="hljs-title function_">addEventListener</span>(<span class="hljs-string">"click"</span>, <span class="hljs-keyword">function</span> (<span class="hljs-params"></span>) {
                <span class="hljs-variable language_">this</span>.<span class="hljs-property">classList</span>.<span class="hljs-title function_">toggle</span>(<span class="hljs-string">"selected"</span>);
                <span class="hljs-title function_">updateMotesFromRewards</span>();
            });

            grid.<span class="hljs-title function_">appendChild</span>(rewardDiv);
        }
    });
}

<span class="hljs-comment">// 🔹 Update Motes from Selected Rewards</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">updateMotesFromRewards</span>(<span class="hljs-params"></span>) {
    <span class="hljs-keyword">let</span> totalMotes = <span class="hljs-number">0</span>;
    <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">querySelectorAll</span>(<span class="hljs-string">".selected .mote-cost"</span>).<span class="hljs-title function_">forEach</span>(<span class="hljs-function"><span class="hljs-params">cost</span> =&gt;</span> {
        totalMotes += <span class="hljs-built_in">parseInt</span>(cost.<span class="hljs-property">textContent</span>) || <span class="hljs-number">0</span>;
    });

    <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(<span class="hljs-string">"motesNeeded"</span>).<span class="hljs-property">textContent</span> = totalMotes;
}
