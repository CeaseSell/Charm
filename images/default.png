<span class="hljs-comment">// Fetch Rewards from Warframe Wiki API</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">fetchRewards</span>(<span class="hljs-params"></span>) {
    <span class="hljs-keyword">const</span> url = <span class="hljs-string">"https://warframe.fandom.com/api.php?action=parse&amp;page=Operation:_Belly_of_the_Beast&amp;format=json&amp;origin=*"</span>;

