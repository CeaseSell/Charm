}

<span class="hljs-comment">// 🔹 Fetch Event &amp; Clan Rewards from Warframe Wiki</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">fetchRewards</span>(<span class="hljs-params"></span>) {
    <span class="hljs-keyword">const</span> proxyUrl = <span class="hljs-string">"https://api.allorigins.win/get?url="</span>;
    <span class="hljs-keyword">const</span> wikiUrl = <span class="hljs-built_in">encodeURIComponent</span>(<span class="hljs-string">"https://warframe.fandom.com/api.php?action=parse&amp;page=Operation:_Belly_of_the_Beast&amp;format=json"</span>);

    <span class="hljs-title function_">fetch</span>(proxyUrl + wikiUrl)
        .<span class="hljs-title function_">then</span>(<span class="hljs-function"><span class="hljs-params">response</span> =&gt;</span> response.<span class="hljs-title function_">json</span>())
        .<span class="hljs-title function_">then</span>(<span class="hljs-function"><span class="hljs-params">data</span> =&gt;</span> {
            <span class="hljs-keyword">if</span> (!data || !data.<span class="hljs-property">contents</span>) {
                <span class="hljs-variable language_">console</span>.<span class="hljs-title function_">error</span>(<span class="hljs-string">"Invalid response from API."</span>);
                <span class="hljs-keyword">return</span>;
            }

            <span class="hljs-keyword">const</span> parsedData = <span class="hljs-title class_">JSON</span>.<span class="hljs-title function_">parse</span>(data.<span class="hljs-property">contents</span>);
            <span class="hljs-keyword">if</span> (!parsedData.<span class="hljs-property">parse</span> || !parsedData.<span class="hljs-property">parse</span>.<span class="hljs-property">text</span>) {
                <span class="hljs-variable language_">console</span>.<span class="hljs-title function_">error</span>(<span class="hljs-string">"Missing 'parse.text' in API response."</span>);
                <span class="hljs-keyword">return</span>;
            }

            <span class="hljs-keyword">const</span> parser = <span class="hljs-keyword">new</span> <span class="hljs-title class_">DOMParser</span>();
            <span class="hljs-keyword">const</span> doc = parser.<span class="hljs-title function_">parseFromString</span>(parsedData.<span class="hljs-property">parse</span>.<span class="hljs-property">text</span>[<span class="hljs-string">"*"</span>], <span class="hljs-string">"text/html"</span>);

            <span class="hljs-comment">// Locate reward tables</span>
            <span class="hljs-keyword">const</span> tables = doc.<span class="hljs-title function_">querySelectorAll</span>(<span class="hljs-string">"table.wikitable"</span>);
            <span class="hljs-variable language_">console</span>.<span class="hljs-title function_">log</span>(<span class="hljs-string">`Found <span class="hljs-subst">${tables.length}</span> tables on the wiki page.`</span>);

            <span class="hljs-keyword">if</span> (tables.<span class="hljs-property">length</span> &gt;= <span class="hljs-number">2</span>) {
                <span class="hljs-title function_">processRewardsTable</span>(tables[<span class="hljs-number">0</span>], <span class="hljs-string">"eventRewards"</span>);
                <span class="hljs-title function_">processRewardsTable</span>(tables[<span class="hljs-number">1</span>], <span class="hljs-string">"clanRewards"</span>);
            } <span class="hljs-keyword">else</span> {
                <span class="hljs-variable language_">console</span>.<span class="hljs-title function_">error</span>(<span class="hljs-string">"No reward tables found on Warframe Wiki."</span>);
            }
        })
        .<span class="hljs-title function_">catch</span>(<span class="hljs-function"><span class="hljs-params">error</span> =&gt;</span> <span class="hljs-variable language_">console</span>.<span class="hljs-title function_">error</span>(<span class="hljs-string">"Error fetching rewards:"</span>, error));
}

<span class="hljs-comment">// 🔹 Process the Reward Tables and Make them Selectable</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">processRewardsTable</span>(<span class="hljs-params">table, containerId</span>) {
    <span class="hljs-keyword">const</span> rows = table.<span class="hljs-title function_">querySelectorAll</span>(<span class="hljs-string">"tr"</span>);
    <span class="hljs-keyword">const</span> tbody = <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(containerId);

    rows.<span class="hljs-title function_">forEach</span>(<span class="hljs-function">(<span class="hljs-params">row, index</span>) =&gt;</span> {
        <span class="hljs-keyword">if</span> (index === <span class="hljs-number">0</span>) <span class="hljs-keyword">return</span>; <span class="hljs-comment">// Skip header row</span>

        <span class="hljs-keyword">const</span> columns = row.<span class="hljs-title function_">querySelectorAll</span>(<span class="hljs-string">"td"</span>);
        <span class="hljs-keyword">if</span> (columns.<span class="hljs-property">length</span> &gt;= <span class="hljs-number">2</span>) {
            <span class="hljs-keyword">const</span> rewardName = columns[<span class="hljs-number">0</span>].<span class="hljs-property">textContent</span>.<span class="hljs-title function_">trim</span>();
            <span class="hljs-keyword">const</span> rewardCost = <span class="hljs-built_in">parseInt</span>(columns[<span class="hljs-number">1</span>].<span class="hljs-property">textContent</span>.<span class="hljs-title function_">trim</span>()) || <span class="hljs-number">0</span>;

            <span class="hljs-keyword">const</span> newRow = <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">createElement</span>(<span class="hljs-string">"tr"</span>);
            newRow.<span class="hljs-property">innerHTML</span> = <span class="hljs-string">`
                &lt;td&gt;<span class="hljs-subst">${rewardName}</span>&lt;/td&gt;
                &lt;td&gt;<span class="hljs-subst">${rewardCost}</span>&lt;/td&gt;
            `</span>;
            newRow.<span class="hljs-property">classList</span>.<span class="hljs-title function_">add</span>(<span class="hljs-string">"selectable-reward"</span>);
            newRow.<span class="hljs-title function_">addEventListener</span>(<span class="hljs-string">"click"</span>, <span class="hljs-keyword">function</span> (<span class="hljs-params"></span>) {
                <span class="hljs-variable language_">this</span>.<span class="hljs-property">classList</span>.<span class="hljs-title function_">toggle</span>(<span class="hljs-string">"selected"</span>);
                <span class="hljs-title function_">updateMotesFromRewards</span>();
            });

            tbody.<span class="hljs-title function_">appendChild</span>(newRow);
        }
    });

    <span class="hljs-comment">// Add Select All / Deselect All Buttons</span>
    <span class="hljs-keyword">const</span> container = <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">getElementById</span>(containerId).<span class="hljs-property">parentElement</span>;
    <span class="hljs-keyword">const</span> buttons = <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">createElement</span>(<span class="hljs-string">"div"</span>);
    buttons.<span class="hljs-property">classList</span>.<span class="hljs-title function_">add</span>(<span class="hljs-string">"reward-controls"</span>);
    buttons.<span class="hljs-property">innerHTML</span> = <span class="hljs-string">`
        &lt;button onclick="selectAll('<span class="hljs-subst">${containerId}</span>')"&gt;Select All&lt;/button&gt;
        &lt;button onclick="deselectAll('<span class="hljs-subst">${containerId}</span>')"&gt;Deselect All&lt;/button&gt;
    `</span>;
    container.<span class="hljs-title function_">prepend</span>(buttons);
}

<span class="hljs-comment">// 🔹 Select All Rewards</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">selectAll</span>(<span class="hljs-params">containerId</span>) {
    <span class="hljs-variable language_">document</span>.<span class="hljs-title function_">querySelectorAll</span>(<span class="hljs-string">`#<span class="hljs-subst">${containerId}</span> tr`</span>).<span class="hljs-title function_">forEach</span>(<span class="hljs-function"><span class="hljs-params">row</span> =&gt;</span> {
