// 📜 **Fetch Image Using Warframe Wiki API**
async function fetchImage(rewardName) {
    const formattedName = encodeURIComponent(rewardName.replace(/\s+/g, "_"));
    const wikiApiUrl = <code>https://warframe.fandom.com/api.php?action=query&amp;format=json&amp;prop=pageimages&amp;pithumbsize=100&amp;titles=${formattedName}&amp;origin=*</code>;

    try {
        const response = await fetch(wikiApiUrl);
        const data = await response.json();

        const pages = data.query.pages;
        const pageId = Object.keys(pages)[0];

        if (pages[pageId].thumbnail) {
            return pages[pageId].thumbnail.source;
        } else {
            return "default.png"; // Placeholder for missing images
        }
    } catch (error) {
        console.error(<code>Failed to fetch image for ${rewardName}:</code>, error);
        return "default.png";
    }
}

