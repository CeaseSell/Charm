/* Global Dark Theme */
body {
    background-color: #1e1e1e;
    color: white;
    font-family: Arial, sans-serif;
    margin: 0;
    padding: 0;
}

/* Center All Content */
#content {
    display: flex;
    flex-direction: column;
    align-items: center;
    text-align: center;
    padding: 20px;
}

/* Tabs Styling */
#tabs {
    display: flex;
    justify-content: center;
    background-color: #3c3c3c;
    padding: 10px;
    border-bottom: 2px solid #555;
}

#tabs button {
    margin-right: 10px;
    padding: 10px;
    border: none;
    cursor: pointer;
    color: white;
    background-color: #444;
    font-size: 16px;
}

#tabs button:hover {
    background-color: #555;
}

/* Tab Content */
.tab {
    display: none;
    width: 80%;
}

.tab.active {
    display: block;
    margin: auto;
}

/* Centered Title Bar */
.belly-title {
    font-size: 24px;
    font-weight: bold;
    background-color: #333;
    color: #9acd32;
    padding: 15px;
    text-align: center;
    width: 80%;
    margin: 20px auto;
    border-radius: 5px;
}

/* Rewards Grid */
.rewards-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
    gap: 10px;
    justify-items: center;
    padding: 20px;
}

/* Reward Item Style */
.reward-item {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background-color: #333;
    padding: 10px;
    border-radius: 5px;
    text-align: center;
    cursor: pointer;
    transition: transform 0.2s, background-color 0.3s;
    width: 150px;
}

.reward-item img {
    width: 128px;
    height: 128px;
    object-fit: contain;
}

.reward-item.selected {
    background-color: #9acd32;
    transform: scale(1.1);
    border: 2px solid #fff;
}

/* Hide Show Button */
#toggleRewards {
    display: block;
    margin: 20px auto;
    background-color: #9acd32;
    color: black;
    border: none;
    padding: 10px 20px;
    cursor: pointer;
    border-radius: 5px;
}

#toggleRewards:hover {
    background-color: #7aaf29;
}

/* Select All / Deselect All Buttons */
.reward-controls {
    display: flex;
    justify-content: center;
    margin: 15px auto;
}

.reward-controls button {
    background-color: #555;
    color: white;
    border: none;
    padding: 5px 15px;
    margin: 5px;
    cursor: pointer;
    border-radius: 5px;
    font-size: 14px;
}

.reward-controls button:hover {
    background-color: #777;
}

