}

<span class="hljs-comment">// 🔹 Update Motes from Selected Rewards</span>
<span class="hljs-keyword">function</span> <span class="hljs-title function_">updateMotesFromRewards</span>(<span class="hljs-params"></span>) {
    <span class="hljs-keyword">let</span> totalMotes = <span class="hljs-number">0</span>;
